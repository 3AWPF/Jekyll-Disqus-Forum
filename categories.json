---
layout: none
---

{
{% for page in site.pages %}
	{% if page.category == true %}
		"{{ page.id }}" : {
			"id": {{ page.id }},
			"title": "{{ page.title }}",
			"url": "{{ site.url }}{{ page.url }}",
			"icon": "{{ page.icon }}"
		}{% unless forloop.last %},{% endunless %}
	{% endif %}
{% endfor %}
}